    "url": "https://github.com/naver/smarteditor2/issues"
  },
  "license": "LGPL-2.1",
  "author": {
    "name": "NAVER corp.",
    "email": "dl_smarteditor2open@navercorp.com"
  },
  "ignore": [
    "**/.*",
    "**/_*",
    "node_modules",
    "bower_components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/naver/smarteditor2.git"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "gitbook-cli": "^2.3.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
